group pierwszy;

int(i,j) ::= <<
MOV A,#<i> ; po raz <j>

>>

add(p1, p2) ::= "<p1> + <p2>... i nie zawaham się go użyć..."
sub(p1, p2) ::= "<p1> - <p2>... i nie zawaham się go użyć..."
mul(p1, p2) ::= "<p1> * <p2>... i nie zawaham się go użyć..."
div(p1, p2) ::= "<p1> / <p2>... i nie zawaham się go użyć..."

dek(n) ::= <<
DD <n>
>>
